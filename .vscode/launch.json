{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Train Pi05 Fold Cloth",
            "type": "debugpy",
            "request": "launch",
            "module": "scripts.train",
            "args": [
                "pi05_fold_cloth",
                "--exp-name=fold_cloth_experiment",
                "--overwrite"
            ],
            "cwd": "${workspaceFolder}",
            "env": {
                "CUDA_VISIBLE_DEVICES": "0",
                "XLA_PYTHON_CLIENT_MEM_FRACTION": "0.9",
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "console": "integratedTerminal",
            "justMyCode": false,
            "python": "${workspaceFolder}/.venv/bin/python"
        },
        {
